@Article{Ulleboe2012,
  Title                    = {The factor structure of {ADHD} in a general population of primary school children.},
  Author                   = {Ulleb\o{}e, Anne Karin and Breivik, Kyrre and Gillberg, Christopher and Lundervold, Astri J. and Posserud, MajBritt},
  Journal                  = {J Child Psychol Psychiatry},
  Year                     = {2012},

  Month                    = {Sep},
