}

@Article{Angold2002,
  Title                    = {Depression scale scores in 8-17-year-olds: effects of age and gender.},
  Author                   = {Angold, Adrian and Erkanli, Alaattin and Silberg, Judy and Eaves, Lindon and Costello, E Jane},
  Journal                  = {Journal of Child Psychology and Psychiatry and Allied Disciplines},
  Year                     = {2002},

  Month                    = {Nov},
  Number                   = {8},
  Pages                    = {1052--1063},
  Volume                   = {43},

  Abstract                 = {The excess of unipolar depression in females emerges in adolescence. However, studies of age effects on depression scale scores have produced divergent estimates of changes from childhood to adolescence.We explored possible reasons for this discrepancy in two large, longitudinal samples of twins and singletons aged 8-17.There were no differences between twins and singletons in their scores on the Short Mood and Feelings Questionnaire (SMFQ), a 13-item self-report depression scale. SMFQ scores for boys fell over this age-range, while those for girls fell from age 9 to age 11 and then increased from age 12 to age 17. The mean scores of girls under 12 and those 12 and over differed by only around one-fifth of a standard deviation. However, given the non-normal distribution of the scores, a cut point that selected the upper 6\% of scores created the expected female:male ratio of 2:1.Implications for future research on adolescent depression are discussed.},
  Institution              = {Department of Psychiatry and Behavioral Sciences, Duke University Medical Center, Durham, NC 27710, USA. Aangold@psych.mc.duke.edu},
  Keywords                 = {Adolescent; Age Factors; Child; Depressive Disorder, diagnosis/epidemiology; Diseases in Twins; Female; Humans; Male; Psychiatric Status Rating Scales; Questionnaires; Sex Factors; Southeastern United States, epidemiology},
  Language                 = {eng},
  Medline-pst              = {ppublish},
  Owner                    = {astri},
  Pmid                     = {12455926},
  Timestamp                = {2013.03.05}
}

@Article{Antshel2010,
  Title                    = {Executive functioning in high-IQ adults with ADHD.},
