  Pii                      = {164/6/942},
  Pmid                     = {17541055},
  Timestamp                = {2013.10.03},
  Url                      = {http://dx.doi.org/10.1176/appi.ajp.164.6.942}
}

@Article{Polanczyk2008b,
  Title                    = {Epidemiologic considerations in attention deficit hyperactivity disorder: a review and update.},
  Author                   = {Guilherme Polanczyk and Peter Jensen},
  Journal                  = {Child Adolesc Psychiatr Clin N Am},
  Year                     = {2008},

  Month                    = {Apr},
  Number                   = {2},
  Pages                    = {245--60, vii},
  Volume                   = {17},

  Abstract                 = {Epidemiologic data on attention deficit hyperactivity disorder (ADHD) are essential for planning health services and implementing strategies of detection and early intervention, with possible substantial benefits on public health. This article addresses methodological aspects of prevalence studies, recent findings on the prevalence of ADHD in childhood and adolescence based on a systematic review, current findings on the persistence of the disorder over time and prevalence in adulthood, and factors associated with ADHD. Evidence from the reviewed literature indicates the importance of methodological aspects in the understanding of epidemiologic findings and the necessity of large-scale cross-national studies. Moreover, governments clearly must direct attention to childhood mental disorders to guarantee a healthy future for their countries.},
  Doi                      = {10.1016/j.chc.2007.11.006},
