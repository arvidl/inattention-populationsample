}

@Article{Soerensen2008,
  Title                    = {Emotional symptoms in inattentive primary school children: a population-based study.},
  Author                   = {Lin S\o{}rensen and Kenneth Hugdahl and Astri Johansen Lundervold},
  Journal                  = {J Atten Disord},
  Year                     = {2008},

