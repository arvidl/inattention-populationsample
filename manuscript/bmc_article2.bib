@Article{Biederman1998,
  Title                    = {Depression in attention deficit hyperactivity disorder (ADHD) children: "true" depression or demoralization?},
  Author                   = {Biederman, J. and Mick, E. and Faraone, S. V.},
  Journal                  = {J Affect Disord},
  Year                     = {1998},

  Month                    = {Jan},
  Number                   = {1-3},
  Pages                    = {113--122},
  Volume                   = {47},

  Abstract                 = {The purpose of this study was to further evaluate the nature of the association between major depression (MD) and attention deficit hyperactivity disorder (ADHD) by examining predictors of persistence of MD attending to issues of familiality, adversity and comorbidity.Four years of follow-up of 76 depressed ADHD children were analyzed using multivariate regression to determine predictors of persistent MD.Bipolar disorder and higher indices of interpersonal problems predicted MD persistence. In contrast, school difficulty and ADHD-associated measures of severity were not associated with persistent MD. Remission from ADHD was also not statistically significantly associated with remission from MD.ADHD and MD had independent and distinct courses, indicating that ADHD-associated MD reflects a depressive disorder and not merely demoralization.This study may have reduced power due to stratification of our group of ADHD boys with persistent and remitting MD.},
  Institution              = {Pediatric Psychopharmacology Unit (ACC 725), Massachusetts General Hospital, Boston 02114, USA.},
  Keywords                 = {Adolescent; Age Factors; Age of Onset; Attention Deficit Disorder with Hyperactivity, diagnosis/epidemiology; Bipolar Disorder, diagnosis/epidemiology; Child; Comorbidity; Depressive Disorder, diagnosis/epidemiology; Follow-Up Studies; Humans; Interpersonal Relations; Male; Psychiatric Status Rating Scales; Research Design; Risk Factors; Severity of Illness Index},
