  Timestamp                = {2013.07.06}
}

@Article{Rucklidge2001,
  Title                    = {Psychiatric, psychosocial, and cognitive functioning of female adolescents with {ADHD}.},
  Author                   = {Rucklidge, J. J. and Tannock, R.},
