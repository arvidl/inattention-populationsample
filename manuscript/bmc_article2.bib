}

@Article{Hinshaw2007,
  Title                    = {Neuropsychological functioning of girls with {A}ttention-{D}eficit {H}yperactivity disorder followed prospectively into adolescence: evidence for continuing deficits?},
  Author                   = {Hinshaw, Stephen P. and Carte, Estol T. and Fan, Catherine and Jassy, Jonathan S. and Owens, Elizabeth B.},
  Journal                  = {Neuropsychology},
  Year                     = {2007},

  Month                    = {Mar},
  Number                   = {2},
  Pages                    = {263--273},
  Volume                   = {21},

  Abstract                 = {The current study prospectively followed girls with attention-deficit/hyperactivity disorder (ADHD), along with a matched comparison sample, 5 years after childhood neuropsychological assessments. Follow-up neuropsychological measures emphasized attentional skills, executive functions, and language abilities. Paralleling childhood findings, the childhood-diagnosed ADHD group displayed moderate to large deficits in executive/attentional performance as well as in rapid naming relative to the comparison group at follow up (M-sub(age) 14.2 years). ADHD-inattentive versus ADHD-combined contrasts were nonsignificant and of negligible effect size, even when a refined, sluggish cognitive tempo subgroup of the inattentive type was examined. Although ADHD versus comparison group differences largely withstood statistical control of baseline demographics and comorbidities, control of childhood IQ reduced executive function differences to nonsignificance. Yet when the subset of girls meeting diagnostic criteria for ADHD in adolescence was compared with the remainder of the participants, neuropsychological deficits emerged even with full statistical control. Overall, childhood ADHD in girls portends neuropsychological and executive deficits that persist for at least 5 years.},
  Doi                      = {10.1037/0894-4105.21.2.263},
  Institution              = {Department of Psychology,University of California, Berkeley, CA 94720-1650, USA. hinshaw@berkeley.edu},
  Keywords                 = {Adolescent; Analysis of Variance; Attention Deficit Disorder with Hyperactivity, epidemiology/physiopathology/psychology; Case-Control Studies; Child; Comprehension, physiology; Disease Progression; Female; Follow-Up Studies; Humans; Neuropsychological Tests; Prospective Studies; Wechsler Scales, statistics /&/ numerical data},
