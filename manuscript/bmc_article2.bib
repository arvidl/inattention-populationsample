}

@Article{Duff2005,
  Title                    = {The relationship between executive functioning and verbal and visual learning and memory.},
  Author                   = {Duff, Kevin and Schoenberg, Mike R. and Scott, James G. and Adams, Russell L.},
  Journal                  = {Arch Clin Neuropsychol},
  Year                     = {2005},

  Month                    = {Jan},
  Number                   = {1},
  Pages                    = {111--122},
  Volume                   = {20},

  Abstract                 = {Executive functions, which include an individual's ability to develop a response set, inhibit behaviors, plan, and reason, likely impact other areas of cognitive functioning, such as learning and memory. The present study examined the relationship between executive functioning and a wide array of standardized, clinical verbal and visual learning and memory measures in 212 patients referred for a neuropsychological evaluation. IQ was also included in the analyses. Results of the canonical correlation analyses indicated that the two cognitive domains shared 55-60\% of variance, and two canonical variates were present. Although causality cannot be inferred, a clear and robust relationship between executive functioning and memory is evident, and clinicians should consider this overlap when interpreting poor performance among these two domains.},
  Doi                      = {10.1016/j.acn.2004.03.003},
  Institution              = {Department of Psychiatry, University of Iowa Hospitals and Clinics 200 Hawkins Drive, Iowa City, Iowa 52242, USA. kevin.duff@uiowa.edu},
