  Institution              = {Department of Epidemiology and Health Promotion, Municipal Health Service, Amsterdam, The Netherlands. adiepenmaat@ggd.amsterdam.nl},
  Keywords                 = {Adolescent; Arm; Child; Computers; Cumulative Trauma Disorders, complications; Depression, complications; Female; Humans; Low Back Pain; Male; Motor Activity; Netherlands; Pain, etiology/psychology; Shoulder Pain; Stress, Psychological, complications},
