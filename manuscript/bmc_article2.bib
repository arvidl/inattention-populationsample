}

@Article{Boee2014,
  Title                    = {Socioeconomic status and child mental health: the role of parental emotional well-being and parenting practices.},
  Author                   = {B\o{}e, Tormod and Sivertsen, B?rge and Heiervang, Einar and Goodman, Robert and Lundervold, Astri J. and Hysing, Mari},
  Journal                  = {J Abnorm Child Psychol},
