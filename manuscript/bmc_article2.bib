@Article{Breslau2011,
  Title                    = {Behavior problems at ages 6 and 11 and high school academic achievement: longitudinal latent variable modeling.},
  Author                   = {Breslau, Naomi and Breslau, Joshua and Miller, Elizabeth and Raykov, Tenko},
  Journal                  = {Psychiatry Res},
