  Pmid                     = {23942827},
  Timestamp                = {2013.10.16},
  Url                      = {http://dx.doi.org/10.1007/s10802-013-9787-z}
}

@Article{Gomez2013c,
  Title                    = {Validity of the {ADHD} Bifactor Model in General Community Samples of Adolescents and Adults, and a Clinic-Referred Sample of Children and Adolescents.},
