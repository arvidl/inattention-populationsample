  Institution              = {Department of Psychology, Pennsylvania State University, University Park 16802, USA. kb2@psu.edu},
  Keywords                 = {Aggression, psychology; Attention; Child; Child Behavior Disorders, complications/diagnosis/prevention /&/ control; Child, Preschool; Cognition Disorders, complications/diagnosis; Ethnic Groups, psychology; Female; Humans; Interpersonal Relations; Male; Peer Group; Sex Factors; Social Behavior Disorders, complications/diagnosis/prevention /&/ control; Wechsler Scales},
