@Article{Danielsson2012,
  Title                    = {Sleep Disturbance and Depressive Symptoms in Adolescence: The Role of Catastrophic Worry.},
  Author                   = {Danielsson, Nanette S. and Harvey, Allison G. and Macdonald, Shane and Jansson-Fr?jmark, Markus and Linton, Steven J.},
  Journal                  = {Journal of Youth and Adolescence},
  Year                     = {2012},

  Month                    = {Sep},

  Abstract                 = {Depression is a common and debilitating disorder in adolescence. Sleep disturbances and depression often co-occur with sleep disturbances frequently preceding depression. The current study investigated whether catastrophic worry, a potential cognitive vulnerability, mediates the relationship between adolescent sleep disturbances and depressive symptoms, as well as whether there are gender differences in this relationship. High school students, ages 16-18, n = 1,760, 49 \% girls, completed annual health surveys including reports of sleep disturbance, catastrophic worry, and depressive symptoms. Sleep disturbances predicted depressive symptoms 1-year later. Catastrophic worry partially mediated the relationship. Girls reported more sleep disturbances, depressive symptoms, and catastrophic worry relative to boys. The results, however, were similar regardless of gender. Sleep disturbances and catastrophic worry may provide school nurses, psychologists, teachers, and parents with non-gender specific early indicators of risk for depression. Several potentially important practical implications, including suggestions for intervention and prevention programs, are highlighted.},
  Doi                      = {10.1007/s10964-012-9811-6},
  Institution              = {Center for Health and Medical Psychology (CHAMP), School of Law, Psychology, and Social Work, ?rebro University, 70182, ?rebro, Sweden, nanette.danielsson@oru.se.},
  Language                 = {eng},
  Medline-pst              = {aheadofprint},
  Owner                    = {astri},
  Pmid                     = {22968332},
  Timestamp                = {2013.03.05},
  Url                      = {http://dx.doi.org/10.1007/s10964-012-9811-6}
}

@Article{Dannlowski2010,
  Title                    = {Emotion specific modulation of automatic amygdala responses by 5-HTTLPR genotype.},
  Author                   = {Dannlowski, Udo and Konrad, Carsten and Kugel, Harald and Zwitserlood, Pienie and Domschke, Katharina and Sch?ning, Sonja and Ohrmann, Patricia and Bauer, Jochen and Pyka, Martin and Hohoff, Christa and Zhang, Weiqi and Baune, Bernhard T. and Heindel, Walter and Arolt, Volker and Suslow, Thomas},
  Journal                  = {Neuroimage},
  Year                     = {2010},

  Month                    = {Nov},
  Number                   = {3},
  Pages                    = {893--898},
  Volume                   = {53},

  Abstract                 = {A functional polymorphism in the serotonin transporter gene (5-HTTLPR) has been reported to modulate amygdala responsiveness to negative environmental cues. However, it remains unclear whether 5-HTTLPR modulates amygdala responses specifically to negative stimuli or rather to emotionally salient stimuli in general. In 44 healthy subjects, amygdala responses to subliminally presented happy and sad facial expressions were assessed by means of fMRI at 3 Tesla. All subjects were genotyped for 5-HTTLPR and the recently discovered 5-HTT rs25531. We observed a robust emotion by genotype group interaction in the right amygdala. Risk allele carriers (S or L(G)) showed similar amygdala responses to happy faces compared to homozygous L(A)L(A) carriers but increased amygdala responses to sad faces. The right amygdala was the only anatomical region across the whole brain demonstrating this interaction at a reasonable threshold. It appears that whereas 5-HTT gene variation modulates automatic amygdala responsiveness to sad faces, no such association was found for happy faces. We conclude that 5-HTTLPR genotype predominantly impacts the central processing predominantly of negative environmental cues but not of emotionally salient stimuli in general.},
  Doi                      = {10.1016/j.neuroimage.2009.11.073},
  Institution              = {Department of Psychiatry, University of M?nster, Albert-Schweitzer-Str. 11, 48149 M?nster, Germany. dannlow@uni-muenster.de},
  Keywords                 = {Adult; Amygdala, physiology; Brain Mapping; Cues; Emotions, physiology; Facial Expression; Female; Genotype; Humans; Magnetic Resonance Imaging; Male; Polymorphism, Single Nucleotide; Serotonin Plasma Membrane Transport Proteins, genetics},
  Language                 = {eng},
  Medline-pst              = {ppublish},
  Owner                    = {astri},
  Pii                      = {S1053-8119(09)01259-2},
  Pmid                     = {19962442},
  Timestamp                = {2015.03.22},
  Url                      = {http://dx.doi.org/10.1016/j.neuroimage.2009.11.073}
}

