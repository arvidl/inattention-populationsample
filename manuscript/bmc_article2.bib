}

@Article{Hinshaw2002,
  Title                    = {Preadolescent girls with attention-deficit/hyperactivity disorder: I. Background characteristics, comorbidity, cognitive and social functioning, and parenting practices.},
  Author                   = {Hinshaw, Stephen P.},
  Journal                  = {J Consult Clin Psychol},
  Year                     = {2002},

  Month                    = {Oct},
