  Institution              = {Department of Clinical Medicine, University of Bergen, Norway. ole.fasmer@psyk.uib.no},
  Keywords                 = {Adolescent; Adult; Aged; Aged, 80 and over; Analgesics, therapeutic use; Attention Deficit Disorder with Hyperactivity, drug therapy/epidemiology; Central Nervous System Stimulants, therapeutic use; Comorbidity; Databases, Factual; Drug Prescriptions; Drug Utilization; Female; Humans; Male; Middle Aged; Migraine Disorders, drug therapy/epidemiology; Norway, epidemiology},
